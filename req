curl http://localhost:3000/users/1
curl -X PUT -H "Content-Type: application/json" -d '{"name":"New Exercise","points":12,"lastUpdated":"2024-09-02T10:00:00Z"}' http://localhost:3000/users/1/habits/101

curl -X POST -H "Content-Type: application/json" \
    -d '{"name":"newuser","pass":"newpassword"}' \
    http://localhost:3000/register


    curl http://localhost:3000/users/

    curl -X PATCH http://localhost:3000/users/1/habits/101/increment


curl -X PATCH -H "Content-Type: application/json" \
    -d '{"name":"New Habit Name"}' \
    http://localhost:3000/users/1/habits/101/name


 curl -X POST -H "Authorization: your_api_key_here" -H "Content-Type: application/json" \
    -d '{"name":"newuser3","pass":"newpassword"}' \
    http://localhost:3000/register